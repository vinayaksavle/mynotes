Programming Questions

int x = 1245
int sum = 0;
while(x!=0){
	
	int y = x%10;
	
	sum +=y;
	x = x/10;
}

Console.Write(sum);

int[] arr1 =[2,3,1,5];

arr1.sort();

Console.Write(arr1[arr1.length-2]);

int max1 = 0;
int max2 = 0;
int temp = 0;
