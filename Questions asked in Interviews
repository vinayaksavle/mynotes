string temp = "aabbccxyz";

char[] c = temp.toCharArray();

for(int i=0; i<c.length;i++){
	if(c[i]== "x"){
		if(c[i+1] == "y"){
			if(c[i+2] == "z"){
				c[i] = "a";
				c[i+1] = "b";
				c[i+2] = "c";
			}
		}
	}
}

Console.Write(c.ToString());

